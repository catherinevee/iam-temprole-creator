{
  "name": "developer-template",
  "permission_tier": "developer",
  "template_content": "{\n  \"Version\": \"2012-10-17\",\n  \"Statement\": [\n    {\n      \"Effect\": \"Allow\",\n      \"Action\": [\n        \"s3:*\"\n      ],\n      \"Resource\": [\n        \"arn:aws:s3:::${projectId}-*\",\n        \"arn:aws:s3:::${projectId}-*/*\"\n      ]\n    },\n    {\n      \"Effect\": \"Allow\",\n      \"Action\": [\n        \"ec2:*\",\n        \"lambda:*\",\n        \"iam:Get*\",\n        \"iam:List*\",\n        \"iam:PassRole\",\n        \"cloudformation:*\",\n        \"rds:*\",\n        \"elasticloadbalancing:*\",\n        \"autoscaling:*\",\n        \"application-autoscaling:*\",\n        \"logs:*\",\n        \"cloudwatch:*\",\n        \"events:*\",\n        \"sqs:*\",\n        \"sns:*\"\n      ],\n      \"Resource\": \"*\"\n    },\n    {\n      \"Effect\": \"Deny\",\n      \"Action\": [\n        \"iam:CreateRole\",\n        \"iam:DeleteRole\",\n        \"iam:AttachRolePolicy\",\n        \"iam:DetachRolePolicy\",\n        \"iam:PutRolePolicy\",\n        \"iam:DeleteRolePolicy\",\n        \"iam:CreateUser\",\n        \"iam:DeleteUser\",\n        \"iam:CreateGroup\",\n        \"iam:DeleteGroup\",\n        \"iam:AttachUserPolicy\",\n        \"iam:DetachUserPolicy\",\n        \"iam:AttachGroupPolicy\",\n        \"iam:DetachGroupPolicy\",\n        \"kms:DeleteKey\",\n        \"kms:ScheduleKeyDeletion\",\n        \"kms:CreateKey\",\n        \"kms:DisableKey\",\n        \"kms:EnableKeyRotation\",\n        \"kms:PutKeyPolicy\"\n      ],\n      \"Resource\": \"*\"\n    }\n  ]\n}",
  "variables": ["projectId"],
  "version": "1.0"
}
